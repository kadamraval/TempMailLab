rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Rule provided by user for debugging.
    // Makes the 'plans' collection publicly readable and all other collections private.
    match /plans/{planId} {
      allow read: if true;
      allow write: if false;
    }
    // Deny all access to any other document path not explicitly matched above.
    match /{document=**} {
      allow read, write: if false;
    }
  }
}