{
  "manifest_version": 3,
  "name": "Temp Mailer",
  "version": "1.0",
  "description": "Generate temporary email addresses on the fly.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "extension/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
